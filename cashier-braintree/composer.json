{
    "name": "thepany/cashier-braintree",
    "description": "Laravel Cashier provides an expressive, fluent interface to Braintree's subscription billing services.",
    "keywords": ["laravel", "braintree", "billing", "cashier"],
    "license": "MIT",
    "authors": [
        {
            "name": "Cristian Gomez",
            "email": "thepany96@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "braintree/braintree_php": "^5.4",
        "dompdf/dompdf": "^1.0.2",
        "illuminate/contracts": "^7.0|^8.0",
        "illuminate/database": "^7.0|^8.0",
        "illuminate/http": "^7.0|^8.0",
        "illuminate/routing": "^7.0|^8.0",
        "illuminate/support": "^7.0|^8.0",
        "illuminate/view": "^7.0|^8.0",
        "moneyphp/money": "^3.3",
        "nesbot/carbon": "^2.0",
        "symfony/http-kernel": "^5.0"
    },
    "require-dev": {
        "ext-json": "*",
        "mockery/mockery": "~1.0",
        "orchestra/testbench": "^5.0|^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Laravel\\Braintree\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laravel\\Braintree\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.x-dev"
        },
        "laravel": {
            "providers": [
                "Laravel\\Braintree\\CashierServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
